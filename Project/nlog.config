<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      autoReload="true">
	<targets async="true">
		<target xsi:type="File" name="ownFile-web"
				fileName="c:\logs\Project-${shortdate}.log"
		  layout="${longdate} | ${uppercase:${level}} | ${logger} | ${callsite} | ${callsite-linenumber} | ${event-properties:item=RoomNumber} | ${message} | ${exception}| ${aspnet-User-Identity} |${machinename} | ${aspnet-Request-IP} |${assembly-version} | ${aspnet-MVC-Action} | ${aspnet-Request-Url} | ${stacktrace}  "  />


		<!-- write to the void aka just remove -->
		<target xsi:type="Null" name="blackhole" />
	</targets>
	<rules>
		<!--Skip Microsoft logs and so log only own logs-->
		<logger name="Microsoft.*" minlevel="Trace" writeTo="blackhole" final="true" />
		<logger name="*" minlevel="Info" writeTo="ownFile-web" />
	</rules>
</nlog>